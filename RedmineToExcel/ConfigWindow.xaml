<Window x:Class="RedmineToExcel.ConfigWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:RedmineToExcel"
        mc:Ignorable="d"
        Title="Config" Height="323.344" Width="642.053">
    <Grid>
        <Label x:Name="label" Content="Redmine URL" HorizontalAlignment="Left" Margin="28,43,0,0" VerticalAlignment="Top"/>
        <Label x:Name="label1" Content="API Key" HorizontalAlignment="Left" Margin="59,74,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="urlTextbox" HorizontalAlignment="Left" Height="23" Margin="130,43,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="421"/>
        <TextBox x:Name="apiTextbox" HorizontalAlignment="Left" Height="23" Margin="130,74,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="421"/>
        <Button x:Name="saveButton" Content="Save" HorizontalAlignment="Left" Margin="500,241,0,0" VerticalAlignment="Top" Width="75" Click="saveButton_Click"/>
        <Button x:Name="cancelButton" Content="Cancel" HorizontalAlignment="Left" Margin="402,241,0,0" VerticalAlignment="Top" Width="75" Click="cancelButton_Click"/>

    </Grid>
</Window>
